{
	"author": {
		"name": "shirogin"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "^7.2.0",
		"bcrypt": "^5.1.1",
		"box-console": "^2.0.0",
		"bullmq": "^5.24.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"ejs": "^3.1.10",
		"envalid": "^8.0.0",
		"express": "^4.21.1",
		"express-rate-limit": "^7.4.1",
		"generate-password": "^1.7.1",
		"googleapis": "^144.0.0",
		"helmet": "^8.0.0",
		"http-status-codes": "^2.3.0",
		"ioredis": "^5.4.1",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.10.0",
		"mongoose": "^8.8.0",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.9.16",
		"pino": "^9.5.0",
		"pino-http": "^10.3.0",
		"swagger-themes": "^1.4.3",
		"swagger-ui-express": "^5.0.1",
		"zod": "^3.23.8"
	},
	"description": "A backend express with typescript template",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bcrypt": "^5.0.2",
		"@types/cookie-parser": "^1.4.7",
		"@types/cors": "^2.8.17",
		"@types/ejs": "^3.1.5",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.12",
		"@types/nodemailer": "^6.4.16",
		"@types/supertest": "^6.0.2",
		"@types/swagger-ui-express": "^4.1.7",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"pino-pretty": "^11.3.0",
		"rimraf": "^6.0.1",
		"supertest": "^7.0.0",
		"tsup": "^8.3.5",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3",
		"vite-tsconfig-paths": "^5.1.0",
		"vitest": "^2.1.4"
	},
	"license": "MIT",
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
			"biome check --apply --no-errors-on-unmatched"
		]
	},
	"main": "index.ts",
	"name": "express-typescript-template",
	"private": true,
	"repository": "jesuph-jobs/doriane-care-back-end",
	"scripts": {
		"build": "tsup",
		"clean": "rimraf dist coverage",
		"dev": "chcp 65001 && tsx watch --clear-screen=false src/index.ts | pino-pretty",
		"dev:linux": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
		"format": "biome format src/",
		"format:fix": "biome format --write src/",
		"lint": "biome check src/",
		"lint:fix": "biome check src/ --fix",
		"lint:fix-unsafe": "biome check src/ --fix --unsafe",
		"prepare": "husky",
		"start": "node dist/index.js",
		"test": "vitest run"
	},
	"tsup": {
		"clean": true,
		"entry": [
			"!src/**/*.test.*",
			"!src/**/__tests__/**",
			"src"
		],
		"sourcemap": true,
		"splitting": false
	},
	"version": "0.0.1"
}
