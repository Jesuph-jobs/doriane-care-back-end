  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
      "biome check --apply --no-errors-on-unmatched"
    ]
  },
  "main": "dist/src/index.js",
  "name": "doriane-care-back-end",
  "private": true,
  "repository": "Jesuph-jobs/doriane-care-back-end",
  "scripts": {
    "build": "tsc --project ./tsconfig.app.json && tsc-alias -p ./tsconfig.app.json",
    "build:windows": "tsc --project ./tsconfig.app.json & tsc-alias -p ./tsconfig.app.json",
    "clean": "rimraf dist coverage",
    "dev": "chcp 65001 && cross-env NODE_ENV=development nodemon --ext .ts,.tsx --exec \"pnpm run build:windows & node . | pino-pretty\"",
    "dev:linux": "NODE_ENV=development nodemon --ext .ts,.tsx --exec \"pnpm run build && node . | pino-pretty\"",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "lint:fix-unsafe": "biome check --fix --unsafe",
    "prepare": "husky",
    "seed": "chcp 65001 && tsx --tsconfig ./tsconfig.seed.json seed/index.ts",
    "seed:user": "chcp 65001 && tsx --tsconfig ./tsconfig.seed.json seed/user.ts",
    "seed:website": "chcp 65001 && tsx --tsconfig ./tsconfig.seed.json seed/website.ts",
    "start": "node .",
    "test": "vitest run"
  },
  "type": "commonjs",
  "version": "0.0.1",
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
